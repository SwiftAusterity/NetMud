@using NetMud.Data.Architectural.EntityBase

@model Material

<h4>Details</h4>
<div class="form-group row">
    @Html.EditorFor(m => m.Name)
    @Html.EditorFor(m => m.Conductive)
    @Html.EditorFor(m => m.Magnetic)
    @Html.EditorFor(m => m.Flammable)
</div>
<div class="form-group row">
    @Html.EditorFor(m => m.Viscosity)
    @Html.EditorFor(m => m.Density)
    @Html.EditorFor(m => m.Mallebility)
    @Html.EditorFor(m => m.Ductility)
    @Html.EditorFor(m => m.Porosity)
</div>
<div class="form-group row">
    @Html.EditorFor(m => m.SolidPoint)
    @Html.EditorFor(m => m.GasPoint)
    @Html.EditorFor(m => m.TemperatureRetention)
</div>
<div class="form-group row">
    @Html.EditorFor(m => m.HelpText)
</div>

<h4>Damage Resistances</h4>
@Html.EditorFor(m => m.Resistance)

<h4>Alloy Composition</h4>
@Html.EditorFor(m => m.Composition)
