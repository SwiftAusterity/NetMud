@using NetMud.Data.NPC

@model Merchandise

<div class="form-group row">
    <a href="#" class="removeRow col-form-label" title="Remove this" onclick="javascript: $(this).parent().remove(); return false;">X</a>
    <div >
        @Html.EditorFor(m => m.Item, new { validItems = ViewData["ValidItems"] })
        @Html.EditorFor(m => m.Quality)
        @Html.EditorFor(m => m.QualityRange)
        @Html.EditorFor(m => m.MarkRate)
    </div>
</div>

