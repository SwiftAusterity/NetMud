@using NetMud.DataStructure.System

@model HashSet<MessagingType>

@{
    var currentCount = Model.Count();
    var rowsName = ViewData.ModelMetadata.PropertyName + "Rows";
}

<div id="@rowsName" class="iterativeContainer">
    <div class="addRowContainer">
        <div class="col-sm-1">
            @Html.DescriptionFor(m => m)
            <a href="#" class="addRow btn" onclick="javascript:  $('#@rowsName').append(newSense); return false;"><span class="addWord">Add Row</span></a>
        </div>
    </div>
    @if (Model.Count() > 0)
    {
        @Html.EditorForMany(e => e, null, 0, "CollectionEnumDropDownListItem")
    }

</div>
<script type="text/javascript">
    var newSense = decodeHtml('@Html.EmptyEditorForMany(e => e, null, currentCount, "CollectionEnumDropDownListItem")');
</script>

