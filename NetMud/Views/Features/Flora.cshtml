@model IEnumerable<NetMud.DataStructure.Base.Supporting.IFlora>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    var tableColumns = new Dictionary<string, Func<dynamic, string>>();
    tableColumns.Add("Name", obj => obj.Name);
    tableColumns.Add("Sunlight Preference", obj => obj.SunlightPreference.ToString());
    tableColumns.Add("Coniferous", obj => obj.Coniferous.ToString());
}

@Html.Partial("ListTable", new NetMud.Models.ListTableViewModel() { Name = "", Columns = tableColumns, Values = Model.OrderBy(obj => obj.Name) })