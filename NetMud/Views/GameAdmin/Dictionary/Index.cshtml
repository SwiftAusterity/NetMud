@model NetMud.Models.Admin.ManageDictionaryViewModel
@{
    ViewBag.Breadcrumbs = new Tuple<string, string[], string[]>("Dictionary", new string[] { "Dashboard" }, new string[] { "/GameAdmin" });

    var linkText = Html.ActionLink("99999999", "Edit", new { id = 99999999 }).ToString();
    var tableColumns = new Dictionary<string, Func<dynamic, string>>();
    tableColumns.Add("Unique Key", obj => linkText.Replace("99999999", obj.UniqueKey));
    tableColumns.Add("Phrase", obj => obj.Name);
    tableColumns.Add("Type", obj => obj.WordType.ToString());
}

@Html.Partial("ConfigListTable", new NetMud.Models.ListTableViewModel() { EntityTypeName = "Dictionary", Columns = tableColumns, PagingModel = Model, Values = Model.CurrentPageOfItems.OrderBy(obj => obj.UniqueKey), IncludeUnapprove = false, IncludeDelete = true })

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
